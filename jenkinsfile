pipeline{
     
     agent any 

     stages{
        stage('sonar quliyt check'){

            agent {
                docker {
                    image 'maven'
                }
            }
            steps {

                script{

                    withSonarQubeEnv(credentiasId: 'sonar-token'){
                        
                        sh 'mvn clean pakcage sonar:sonar'
                    }
                }
            }
        }
     }
}
